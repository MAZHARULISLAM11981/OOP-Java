
package T79_Parameterised_Method;

public class Teacher {
    String Name,Gender,Email,Phone;
    
    void Information(String Name,String Gender,String Email,String Phone){
       this.Name = Name;
       this.Gender = Gender;
       this.Email = Email;
       this.Phone = Phone;
    }
    void Display(){
         System.out.println("Name : "+Name);
        System.out.println("Email : "+Email);
        System.out.println("Gender : "+Gender);
        System.out.println("Phone : "+Phone);
    }
    
}

package T79_Parameterised_Method;

public class Main {
    public static void main(String[] args) {
        Teacher teacher1= new Teacher();
        teacher1.Information("Md Mazharul Islam Bhuiyan","Male","bhuiyanmdmazharulislam@gmail.com","01818500151");
        teacher1.Display();
        System.out.println("\n");
        Teacher teacher2= new Teacher();
        teacher2.Information("Md Mazharul Islam Bhuiyan","Male","bhuiyanmdmazharulislam@gmail.com","01818500151");
        teacher2.Display();
    }
    
}
